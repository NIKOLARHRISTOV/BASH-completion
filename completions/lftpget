# lftpget(1) completion                                    -*- shell-script -*-

_lftpget()
{
    local cur prev words cword comp_args
    _comp_initialize -- "$@" || return

	if [[ $cur == -* ]]; then
		COMPREPLY=($(compgen -W '-c -d -v' -- "$cur"))
	fi
} &&
	complete -F _lftpget lftpget

# ex: filetype=sh
