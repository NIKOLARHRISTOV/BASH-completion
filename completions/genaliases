# mailman genaliases completion                            -*- shell-script -*-

_genaliases() {
	local cur prev words cword comp_args
	_comp_initialize -- "$@" || return

	if [[ $cur == -* ]]; then
		COMPREPLY=($(compgen -W '--quiet --help' -- "$cur"))
	fi

} &&
	complete -F _genaliases genaliases

# ex: filetype=sh
